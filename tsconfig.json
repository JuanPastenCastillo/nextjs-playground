{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/challenges-useReducer/[slug].js",
    "hooks/useWidthSize.js",
    "utils/testingType.test.ts",
    "utils/my-test.test.js",
    "utils/test-debug.test.js",
    "utils/toBeTypeOf.test.js",
    "utils/returns.test.js",
    "utils/parameters.test.js",
    "utils/parameters.test.js",
    "components/navBar/__tests__/INDEX.test.js",
    "assets/Norway.js",
    "components/static-to-test/dialog.jsx",
    "components/static-to-test/__tests__/INDEX_StaticToTest.test.ts",
    "components/static-to-test/INDEX_StaticToTest.tsxx",
    "components/static-to-test/INDEX_StaticToTest.jsx",
    "utils/user-event/midudev/calculator.test.jsx",
    "utils/user-event/clear.test.jsx",
  ],
  "exclude": [
    "node_modules"
  ],
  "types": [
    "vitest/importMeta"
  ]
}